        "es2021": true,
        "node": true
    },
    "extends": ["airbnb", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"]
    },
}
